<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping auto-import="false" default-lazy="false">
	<import class="com.liferay.data.migration.tool.model.EntityManager" />
	<import class="com.liferay.data.migration.tool.model.Migration" />
	<class name="com.liferay.data.migration.tool.model.impl.EntityManagerImpl" table="SYNC_EntityManager">
		<id name="entityName" type="java.lang.String">
			<generator class="assigned" />
		</id>
		<property name="lastSyncDate" type="org.hibernate.type.TimestampType" />
	</class>
	<class name="com.liferay.data.migration.tool.model.impl.MigrationImpl" table="SYNC_Migration">
		<id name="migrationId" type="long">
			<generator class="assigned" />
		</id>
		<property name="fromDate" type="org.hibernate.type.TimestampType" />
		<property name="timeStarted" type="org.hibernate.type.TimestampType" />
		<property name="timeCompleted" type="org.hibernate.type.TimestampType" />
		<property name="recordsSynced" type="com.liferay.portal.dao.orm.hibernate.LongType" />
	</class>
</hibernate-mapping>